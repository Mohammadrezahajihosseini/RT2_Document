        <span class="bp">self</span><span class="o">.</span><span class="n">data</span> <span class="o">=</span> <span class="p">{</span><span class="s1">&#39;Reached Goals&#39;</span><span class="p">:</span> <span class="bp">self</span><span class="o">.</span><span class="n">goal_counter</span><span class="p">,</span> 
