<div class=" highlight hl-ipython3"><pre><span></span><span class="k">class</span> <span class="nc">Goal_Visualiser</span><span class="p">:</span>
    
    <span class="k">def</span> <span class="fm">__init__</span><span class="p">(</span><span class="bp">self</span><span class="p">):</span>
        
        <span class="c1">#Initial action plot to print Goal status</span>
        <span class="bp">self</span><span class="o">.</span><span class="n">achieved</span> <span class="o">=</span> <span class="kc">False</span>
        <span class="bp">self</span><span class="o">.</span><span class="n">goal_counter</span> <span class="o">=</span> <span class="mi">0</span>
        <span class="bp">self</span><span class="o">.</span><span class="n">non_reached_goal_counter</span> <span class="o">=</span> <span class="mi">0</span>
