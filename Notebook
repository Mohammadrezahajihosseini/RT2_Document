<div class=" highlight hl-ipython3"><pre><span></span><span class="c1">#Environment similar to Rviz to view robot status</span>
<span class="n">v</span> <span class="o">=</span> <span class="n">ros3d</span><span class="o">.</span><span class="n">Viewer</span><span class="p">()</span>
<span class="n">rc</span> <span class="o">=</span> <span class="n">ros3d</span><span class="o">.</span><span class="n">ROSConnection</span><span class="p">(</span><span class="n">url</span><span class="o">=</span><span class="s2">&quot;ws://localhost:9090&quot;</span><span class="p">)</span>
<span class="n">tf_client</span> <span class="o">=</span> <span class="n">ros3d</span><span class="o">.</span><span class="n">TFClient</span><span class="p">(</span><span class="n">ros</span><span class="o">=</span><span class="n">rc</span><span class="p">,</span> <span class="n">fixed_frame</span><span class="o">=</span><span class="s1">&#39;map&#39;</span><span class="p">)</span>
<span class="n">laser_view</span> <span class="o">=</span> <span class="n">ros3d</span><span class="o">.</span><span class="n">LaserScan</span><span class="p">(</span><span class="n">topic</span><span class="o">=</span><span class="s2">&quot;/scan&quot;</span><span class="p">,</span> <span class="n">ros</span><span class="o">=</span><span class="n">rc</span><span class="p">,</span> <span class="n">tf_client</span><span class="o">=</span><span class="n">tf_client</span><span class="p">)</span>
<span class="n">map_view</span> <span class="o">=</span> <span class="n">ros3d</span><span class="o">.</span><span class="n">OccupancyGrid</span><span class="p">(</span><span class="n">topic</span><span class="o">=</span><span class="s2">&quot;/map&quot;</span><span class="p">,</span> <span class="n">ros</span><span class="o">=</span><span class="n">rc</span><span class="p">,</span> <span class="n">tf_client</span><span class="o">=</span><span class="n">tf_client</span><span class="p">)</span>
<span class="n">path</span> <span class="o">=</span> <span class="n">ros3d</span><span class="o">.</span><span class="n">Path</span><span class="p">(</span><span class="n">topic</span><span class="o">=</span><span class="s2">&quot;/move_base/NavfnROS/plan&quot;</span><span class="p">,</span> <span class="n">ros</span><span class="o">=</span><span class="n">rc</span><span class="p">,</span>
<span class="n">tf_client</span><span class="o">=</span><span class="n">tf_client</span><span class="p">)</span>
<span class="n">urdf</span> <span class="o">=</span> <span class="n">ros3d</span><span class="o">.</span><span class="n">URDFModel</span><span class="p">(</span><span class="n">ros</span><span class="o">=</span><span class="n">rc</span><span class="p">,</span> <span class="n">tf_client</span><span class="o">=</span><span class="n">tf_client</span><span class="p">,</span>
<span class="n">path</span><span class="o">=</span><span class="n">os</span><span class="o">.</span><span class="n">environ</span><span class="o">.</span><span class="n">get</span><span class="p">(</span><span class="s1">&#39;JUPYROS_ASSETS_URL&#39;</span><span class="p">,</span> <span class="s1">&#39;http://localhost:3000&#39;</span><span class="p">))</span>
<span class="n">g</span> <span class="o">=</span> <span class="n">ros3d</span><span class="o">.</span><span class="n">GridModel</span><span class="p">()</span>
<span class="n">v</span><span class="o">.</span><span class="n">objects</span> <span class="o">=</span> <span class="p">[</span><span class="n">g</span><span class="p">,</span> <span class="n">laser_view</span><span class="p">,</span> <span class="n">map_view</span><span class="p">,</span> <span class="n">path</span><span class="p">,</span> <span class="n">urdf</span><span class="p">]</span>
